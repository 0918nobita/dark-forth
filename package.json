{
  "name": "forth-book",
  "version": "1.0.0",
  "scripts": {
    "serve": "gitbook build ./dark-forth && gitbook --port 8080 serve ./dark-forth",
    "pdf": "gitbook pdf ./dark-forth ./dark-forth.pdf",
    "epub": "gitbook epub ./dark-forth ./dark-forth.epub",
    "build": "gitbook build ./dark-forth && rm -rf docs && cp -r ./dark-forth/_book docs"
  },
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "gitbook": "^2.6.9",
    "gitbook-cli": "^2.3.2",
    "svgexport": "^0.3.2"
  }
}
